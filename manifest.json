{
  "manifest_version": 2,

  "name": "TabCleaner"",
  "description": "aids in the closing of large groups of tabs",
  "version": ".1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "settings.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
